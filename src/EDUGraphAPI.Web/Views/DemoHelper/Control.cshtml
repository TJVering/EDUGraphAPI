@model DemoPage

<div class="demo-helper-control collapsed">
    <div class="header">
        DEMO HELPER
    </div>
    <div class="header-right-shadow-mask"></div>
    <div class="body">
        <p>Here are some links to the code samples that you can download and try for yourself:</p>
        @if (Model != null)
        {
            <ul>
                @foreach (var link in Model.Links)
                {
                    <li>
                        <p class="title">@link.Title</p>
                        <p><a href="@link.Url" target="_blank">@link.Url</a></p>
                    </li>
                }
            </ul>
        }
        else
        {
            <p class="empty-result">No link was found for this page.</p>
        }
    </div>
</div>